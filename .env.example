# 环境配置
ENV=development  # development, production, testing
DEBUG=true

# API密钥配置
API_KEYS=["AIzaSyxxxxxxxxxxxxxxxxxxx","AIzaSyxxxxxxxxxxxxxxxxxxx"]
ALLOWED_TOKENS=["sk-123456"]
# AUTH_TOKEN=sk-123456  # 如果不设置，将使用ALLOWED_TOKENS中的第一个值
MAX_FAILURES=10
PAID_KEY=AIzaSyxxxxxxxxxxxxxxxxxxx

# API服务配置
BASE_URL=https://generativelanguage.googleapis.com/v1beta
MODEL_SEARCH=["gemini-2.0-flash-exp","gemini-2.0-pro-exp"]
TOOLS_CODE_EXECUTION_ENABLED=true
SHOW_SEARCH_LINK=true
SHOW_THINKING_PROCESS=true
TEST_MODEL=gemini-1.5-flash

# 图像生成配置
CREATE_IMAGE_MODEL=imagen-3.0-generate-002
UPLOAD_PROVIDER=smms
SMMS_SECRET_TOKEN=XXXXXXXXXXXXXXXXXXXXXXXXXXXXXX

# CORS配置
# CORS_ALLOWED_ORIGINS=["https://example.com","https://api.example.com"]

# 性能配置
REQUEST_TIMEOUT=300  # 请求超时时间（秒）
MAX_RETRIES=3        # 最大重试次数

# MySQL数据库配置
MYSQL_HOST=localhost
MYSQL_PORT=3306
MYSQL_USER=user
MYSQL_PASSWORD=password
MYSQL_DATABASE=gemini
# 或者直接配置完整的DATABASE_URL
# DATABASE_URL=mysql+aiomysql://user:password@localhost:3306/gemini

# 日志配置
LOG_LEVEL=INFO  # DEBUG, INFO, WARNING, ERROR, CRITICAL
